{
  "name": "typescript-node-template",
  "version": "1.0.0",
  "description": "Typescript node template with jest and express",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "generate": "npx prisma generate",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "build": "rimraf ./dist && tsc",
    "dev": "nodemon -r dotenv/config index.ts",
    "dev:build": "npm-run-all -s build dev",
    "start": "npm run build && node dist/index.js",
    "migrate": "npx prisma migrate dev --name init",
    "reset": "npx prisma migrate reset",
    "lint:fix": "eslint . --fix",
    "seed": "ts-node ./prisma/seed/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SharjeelSIqbal/typescript-node-template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SharjeelSIqbal/typescript-node-template/issues"
  },
  "homepage": "https://github.com/SharjeelSIqbal/typescript-node-template#readme",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.3.3",
    "@types/supertest": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prisma": "^5.0.0",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "vite": "^4.4.11"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed/index.ts"
  }
}

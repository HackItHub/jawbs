{
  "name": "flash-resume",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npm-run-all -s lint:server lint:client",
    "prepare": "husky install",
    "test:client": "cd flash-resume-client && npm run test",
    "lint:server": "cd flash-resume-server && eslint .",
    "lint:client": "cd flash-resume-client && eslint .",
    "dev:client": "cd flash-resume-client && npm run dev",
    "dev:server": "cd flash-resume-server && npm run dev",
    "dev": "npm-run-all -p dev:server dev:client",
    "setup": "npm-run-all -s setup:client setup:server",
    "setup:client": "cd flash-resume-client && npm i",
    "setup:server": "cd flash-resume-server && npm i",
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down",
    "docker:dev:build": "docker compose -f docker-compose.dev.yml up --build",
    "lint:fix:client": "cd flash-resume-client && eslint . --fix",
    "lint:fix:server": "cd flash-resume-server && eslint . --fix",
    "lint:fix": "npm-run-all -s lint:fix:client lint:fix:server",
    "docker:prune:system": "docker system prune -a",
    "docker:prune:volume": "docker volume rm $(docker volume ls -q)",
    "docker:prune": "npm-run-all -s docker:prune:system docker:prune:volume",
    "docker:rebuild": "npm-run-all -s docker:prune docker:dev:build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "husky": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8"
  }
}
